void print(int n) {
	int i,j,m,k;
	i = 1;
	while(i <= 2 * n - 1){
		k=i;
		m=n;
		if(i > n) {
			k = 2 * n - i;
		}
		j = 1;
		while(j <= n - k){
			out "  ";
			j = j + 1;
		}
		j = 1;
		while(j <= 1 + 2 * (k - 1)) {
			if(j > (1 + 2 * (k - 1)) / 2) {
				if (m < 10) {
					out " ";
				}
				out m;
				m = m + 1;
				j = j + 1;
			}
			else {
				if (m < 10) {
					out " ";
				}
				out m;
				m = m - 1;
				j = j + 1;
			}
		}
		out "\n";
		i = i + 1;
	}
}

void print(int n, string s) {
	int i,j;
    for(i = 1; i <= n; i = i + 1) {
        for(j = n-1; j >= i; j = j - 1) {
			out " ";
        }
        for(j = 1; j <= 2 * i - 1; j = j + 1) {
			out s;
        }
        out "\n";
    }
    for(i = n - 1; i >= 1; i = i - 1) {
		for(j=i;j<=n-1;j = j + 1) {
			out " ";
		}
		for(j = 1; j <= 2 * i - 1; j = j + 1) {
			out s;
		}
		out "\n";
    }
}